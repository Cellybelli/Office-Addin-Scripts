    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Command Line: clear",
            "type": "node",
            "request": "launch",            
            "program": "${workspaceFolder}/lib/dev-settings.js",
            "args": [
                "clear", "${workspaceFolder}\\test\\manifest.xml"
            ]
        },
        {
            "name": "Command Line: disable-debugging",
            "type": "node",
            "request": "launch",            
            "program": "${workspaceFolder}/lib/dev-settings.js",
            "args": [
                "disable-debugging", "${workspaceFolder}\\test\\manifest.xml"
            ]
        },
        {
            "name": "Command Line: enable-debugging",
            "type": "node",
            "request": "launch",            
            "program": "${workspaceFolder}/lib/dev-settings.js",
            "args": [
                "enable-debugging", "${workspaceFolder}\\test\\manifest.xml"
            ]
        }
    ]
}