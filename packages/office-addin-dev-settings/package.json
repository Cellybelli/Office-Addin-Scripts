{
  "name": "office-addin-dev-settings",
  "version": "0.0.1",
  "description": "Configure developer settings for Office Add-ins.",
  "main": "./lib/dev-settings.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-dev-settings": "./lib/dev-settings.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "child_process": "^1.0.2"
  },
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/node": "^10.5.7",
    "@types/node-fetch": "^2.1.2",
    "concurrently": "^3.5.0",
    "rimraf": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
