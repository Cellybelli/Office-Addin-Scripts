{
  "name": "office-addin-manifest",
  "version": "0.0.1",
  "description": "Read and modify Office Add-in manifest files.",
  "main": "./lib/manifest.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tsc-watch": "tsc -p tsconfig.json -w",
    "build": "rimraf lib && concurrently \"npm run tsc\"",
    "start": "rimraf lib && concurrently \"npm run tsc-watch\"",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "author": "Office Dev",
  "license": "MIT",
  "bin": {
    "office-addin-manifest": "./lib/manifest.js"
  },
  "keywords": [
    "Office",
    "Office Add-in"
  ],
  "dependencies": {
    "commander": "^2.18.0",
    "fs": "0.0.1-security",
    "node-fetch": "^2.2.0",
    "path": "^0.12.7",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "@types/node-fetch": "^2.1.2",
    "@types/xml2js": "^0.4.3",
    "concurrently": "^3.6.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts"
  },
  "bugs": {
    "url": "https://github.com/OfficeDev/Office-Addin-Scripts/issues"
  }
}
